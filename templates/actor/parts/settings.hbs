                        <input class="inline-edit" data-field="system.main.levels" type="number" value="{{class.system.main.levels}}" />
                        <a class="item-expand {{#if system.expand}}showOpacity{{/if}}"><i class="{{#if system.expand}}fas fa-caret-up{{else}}fas fa-caret-down{{/if}}"></i></a>
                    </div>
                    {{#if system.expand}}
                    <span> {{localize "abfalter.settingsTab.tempWip"}} </span>
                    {{/if}}
                </div>
                {{/each}}
            </div>

            <div>
                <a class="item-control item-create" data-type="class"><i class="fas fa-plus"></i> {{localize "abfalter.settingsTab.addClass"}} </a>
                <a class="toggleBoolean {{#if system.itemToggle.class}}armorpenalty{{/if}}" data-label="system.itemToggle.class" data-ability="{{system.itemToggle.class}}"><i class="{{#if system.itemToggle.class}}fa-solid fa-bars-staggered{{else}}fa-solid fa-bars{{/if}}"></i> {{localize "abfalter.modify"}} </a>
            </div>
        </div>


        <div class="homebrew">
            <h5> {{localize "abfalter.settingsTab.homebrewSettings"}} </h5>
            <label>
                <span> {{localize "abfalter.settingsTab.increasedStr"}} </span>
                <button type="button" class="toggleBoolean {{#if system.toggles.psychicStrengthening}}kiBought{{/if}}"
                        data-label="system.toggles.psychicStrengthening" data-ability="{{system.toggles.psychicStrengthening}}">
                    {{#if system.toggles.psychicStrengthening}}
                    <span> {{localize "abfalter.settingsTab.active"}} </span>
                    {{else}}
                    <span> {{localize "abfalter.settingsTab.notActive"}} </span>
                    {{/if}}
                </button>
            </label>
        </div>

    </div>

</div>
