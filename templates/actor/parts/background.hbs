<div class="tab background">
    <div class="e2">
        <div class="elan">
            <h5> {{localize "abfalter.generalTab.elan"}} </h5>
            <div>
                <span> {{localize "abfalter.name"}} </span>
                <span> {{localize "abfalter.generalTab.title"}} </span>
                <span> {{localize "abfalter.generalTab.elan"}} </span>
            </div>
            <div class="{{#if system.itemToggle.elan}}item-list{{/if}}">
                {{#each elans as |elan id|}}
                <div class="elan-item item normal-item" data-item-id="{{elan._id}}">
                    {{#if system.expand}}
                    <div>
                        <input class="item-name inline-edit" data-field="name" type="text" value="{{elan.name}}" placeholder="{{localize "abfalter.sheet.newItem"}}" />
                        <input class="inline-edit" data-field="system.title" type="text" value="{{elan.system.title}}" />
                        <input class="inline-edit" data-field="system.level" type="number" value="{{elan.system.level}}" />
                        <a class="item-expand {{#if system.expand}}showOpacity{{/if}}"><i class="{{#if system.expand}}fas fa-caret-up{{else}}fas fa-caret-down{{/if}}"></i></a>
                    </div>
                    <div style="display: grid;">
                        <textarea class="inline-edit textarea-auto-resize" data-field="system.description" style="grid-area: 1 / 1 / 2/ 2;">{{elan.system.description}}</textarea>
                        <div class="textarea-auto-resize-sibling" aria-hidden="true" style="visibility: hidden; grid-area: 1 / 1 / 2 / 2;">{{elan.system.description}}</div>
                    </div>
                    <div>
                        <div>
                            {{#if system.upper}}
                            <h6> {{localize "abfalter.generalTab.gainUpperElan"}} </h6>
                            <div style="display: grid;">
                                <textarea class="inline-edit textarea-auto-resize" data-field="system.gainUpper" style="grid-area: 1 / 1 / 2/ 2;">{{elan.system.gainUpper}}</textarea>
                                <div class="textarea-auto-resize-sibling" aria-hidden="true" style="visibility: hidden; grid-area: 1 / 1 / 2 / 2;">{{elan.system.gainUpper}}</div>
                            </div>
                            {{else}}
                            <h6> {{localize "abfalter.generalTab.gainElan"}} </h6>
                            <div style="display: grid;">
                                <textarea class="inline-edit textarea-auto-resize" data-field="system.gain" style="grid-area: 1 / 1 / 2/ 2;">{{elan.system.gain}}</textarea>
                                <div class="textarea-auto-resize-sibling" aria-hidden="true" style="visibility: hidden; grid-area: 1 / 1 / 2 / 2;">{{elan.system.gain}}</div>
                            </div>
                            {{/if}}
                        </div>
                        <div>
                            <h6> {{localize "abfalter.generalTab.loseElan"}} </h6>
                            <div style="display: grid;">
                                <textarea class="inline-edit textarea-auto-resize" data-field="system.lose" style="grid-area: 1 / 1 / 2/ 2;">{{elan.system.lose}}</textarea>
                                <div class="textarea-auto-resize-sibling" aria-hidden="true" style="visibility: hidden; grid-area: 1 / 1 / 2 / 2;">{{elan.system.lose}}</div>
                            </div>
                        </div>
                    </div>
                    {{else}}
                    <div>
                        <span> {{elan.name}} </span>
                        <span> {{elan.system.title}} </span>
                        <input class="inline-edit" data-field="system.level" type="number" value="{{elan.system.level}}" />
                        <a class="item-expand {{#if system.expand}}showOpacity{{/if}}"><i class="{{#if system.expand}}fas fa-caret-up{{else}}fas fa-caret-down{{/if}}"></i></a>
                    </div>
                    {{/if}}
                </div>
                {{/each}}
            </div>
            <div class="itemControls">
                <a class="item-control item-create" data-type="elan"><i class="fas fa-plus"></i> {{localize "abfalter.generalTab.addElan"}} </a>
                <a class="toggleBoolean {{#if system.itemToggle.elan}}armorpenalty{{/if}}" data-label="system.itemToggle.elan" data-ability="{{system.itemToggle.elan}}"><i class="{{#if system.itemToggle.elan}}fa-solid fa-bars-staggered{{else}}fa-solid fa-bars{{/if}}"></i> {{localize "abfalter.sheet.modify"}} </a>
            </div>
        </div>

        <div class="natura">
            <h5> {{localize "abfalter.natura"}} </h5>
            <div>
                <label> <span> {{localize "abfalter.destiny"}} </span> <input type="number" name="system.info.destiny" value="{{system.info.destiny}}" /> </label>
            </div>
        </div>
    </div>

